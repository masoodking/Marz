                "parameters": [
                    {
                        "name": "phone_number",
                        "in": "query",
                        "description": "user phone number.",
                        "required": true,
                        "type": "number",
                        "format": "string"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "user password.",
                        "required": true,
                        "type": "string",
                        "format": "string"
                    }
                ],
                "tags": [
                    "User"
                ],
                "responses": {
                    "200": {
                        "description": "عملیات سرور به درستی انجام شده و یک پیام در قسمت message قرار می‌گیرد",
